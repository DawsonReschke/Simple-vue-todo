r.setRequestHeader('Content-Type', 'application/json');\n            xhr.send(JSON.stringify(data)); \n            this.resetForm();\n        },\n        resetForm(){\n            this.title = \"\";\n            this.description = \"\";\n            this.categoryValue = \"\";        \n        }\n    },\n}\n</script>\n\n<style scoped>\n</style>"],"sourceRoot":"src/components"}]}